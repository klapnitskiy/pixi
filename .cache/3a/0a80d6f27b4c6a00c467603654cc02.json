ll,"original":{"line":58,"column":0},"generated":{"line":42,"column":0}},{"source":"../../src/loaders/loadDDS.ts","name":null,"original":{"line":58,"column":0},"generated":{"line":42,"column":16}},{"source":"../../src/loaders/loadDDS.ts","name":null,"original":{"line":58,"column":11},"generated":{"line":42,"column":17}},{"source":"../../src/loaders/loadDDS.ts","name":null,"original":{"line":58,"column":11},"generated":{"line":42,"column":20}},{"source":"../../src/loaders/loadDDS.ts","name":null,"original":{"line":58,"column":15},"generated":{"line":42,"column":21}},{"source":"../../src/loaders/loadDDS.ts","name":null,"original":{"line":58,"column":22},"generated":{"line":42,"column":28}},{"source":"../../src/loaders/loadDDS.ts","name":null,"original":{"line":58,"column":22},"generated":{"line":42,"column":29}}],"sources":{"../../src/loaders/loadDDS.ts":"import { checkExtension, createTexture, LoaderParserPriority } from 'pixijs/assets';\nimport { ALPHA_MODES, BaseTexture, extensions, ExtensionType, MIPMAP_MODES, settings, utils } from 'pixijs/core';\nimport { parseDDS } from '../parsers';\n\nimport type { LoadAsset, Loader, LoaderParser } from 'pixijs/assets';\nimport type { IBaseTextureOptions, Texture } from 'pixijs/core';\n\n/** Load our DDS textures! */\nexport const loadDDS: LoaderParser = {\n    extension: {\n        type: ExtensionType.LoadParser,\n        priority: LoaderParserPriority.High,\n    },\n\n    test(url: string): boolean\n    {\n        return checkExtension(url, '.dds');\n    },\n\n    async load(url: string, asset: LoadAsset, loader: Loader): Promise<Texture | Texture[]>\n    {\n        // get an array buffer...\n        const response = await settings.ADAPTER.fetch(url);\n\n        const arrayBuffer = await response.arrayBuffer();\n\n        const resources = parseDDS(arrayBuffer);\n\n        const textures = resources.map((resource) =>\n        {\n            const base = new BaseTexture(resource, {\n                mipmap: MIPMAP_MODES.OFF,\n                alphaMode: ALPHA_MODES.NO_PREMULTIPLIED_ALPHA,\n                resolution: utils.getResolutionOfUrl(url),\n                ...asset.data,\n            });\n\n            return createTexture(base, loader, url);\n        });\n\n        return textures.length === 1 ? textures[0] : textures;\n    },\n\n    unload(texture: Texture | Texture[]): void\n    {\n        if (Array.isArray(texture))\n        {\n            texture.forEach((t) => t.destroy(true));\n        }\n        else\n        {\n            texture.destroy(true);\n        }\n    }\n\n} as LoaderParser<Texture | Texture[], IBaseTextureOptions>;\n\nextensions.add(loadDDS);\n"},"lineCount":null}},"error":null,"hash":"e5cfdce39cccc68d2fbc163f36d64971","cacheData":{"env":{}}}